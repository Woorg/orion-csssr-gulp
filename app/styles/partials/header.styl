.header
	position relative
	clearfix()
	&-top
		position absolute
		top 0
		left 0
		width 100%
		z-index 52
	&-top-in
		position relative
		@extend .in
		padding 1.1875rem 0 1.125rem
		min-height 4.375rem
		font normal 0.875rem/1 $ff-mb
		text-transform uppercase
	&-logo
		margin-top 0.1875rem
		display block
		float left
		sprite($logo)
		hide-text()
		@media all and (-webkit-min-device-pixel-ratio: 1.5)
			sprite2($logo_2x)
	&-nav
		margin-right 6.5rem
		display block
		float right
		@media all and (max-width 20rem)
			margin-right 5rem
		@media all and (min-width 47.5rem)
			min-width 30.625rem
		@media all and (min-width 53.75rem)
			min-width 33.125rem
		@media all and (min-width 61.8125rem)
			min-width 37.5rem
		&-ul
			margin 0
			padding 0
			list-style none
			font-size 0
			@media all and (max-width 20rem)
				background #3498db
				position absolute
				top 3.75rem
				right 0
				-webkit-border-radius 0.3125rem
				border-radius 0.3125rem
				display: none
				&.is-active
						display: block
				&-i
					display block
			@media all and (min-width 20.0625rem)
				position absolute
				top 3.75rem
				right 0
				background #3498db
				-webkit-border-radius 0.3125rem
				border-radius 0.3125rem
				display: none
				&.is-active
					display: block
				&-i
					display block
			@media all and (min-width 47.5rem)
				position static
				display block
				background-color transparent
			&.js-nav_btn
				display block

		&-i
			margin-left (33/989) * 100%
			display inline-block
			line-height 2.0625rem
			font-size 0.875rem
			&:first-child
				margin-left 0
			a
				color #FFF
				text-shadow 0 0.125rem 0.0625rem #656565
			@media all and (max-width 20rem)
				margin 0
				padding 0 1.25rem
				display block
			@media all and (min-width 20.0625rem)
				margin 0
				padding 0 1.25rem
				display block
			@media all and (min-width 47.5rem)
				display inline-block
			@media all and (min-width 47.5625rem)
				margin-left (24/989) * 100%
				padding: 0
			@media all and (min-width 53.75rem)
				margin-left (33/989) * 100%
		&-button
			display: none
			transition all .3 ease
			cursor pointer
			&:hover
				background-color #59a3fc
			@media all and (max-width 20rem)
				position absolute
				top 1.375rem
				right 5rem
				display inline-block
			@media all and (min-width 20.0625rem)
				position absolute
				top 1.375rem
				right 5rem
			@media all and (min-width 47.5rem)
				display none
	&-top-control
		position absolute
		top 1.25rem
		right 0
		@media all and (max-width 20rem)
			top 1.375rem
		@media all and (min-width 20.0625rem)
			top 1.375rem
	&-slider
		height 43.0625rem
		background #2076dd
		overflow hidden
	&-body
		padding-top 4.375rem
		padding-bottom 4.3125rem
		height 43.0625rem
		text-align center
		color #FFF
		&._style-1
			background #2076dd
			background-image url('../images/low-res/header-img.jpg')
			background-size cover
			background-position 50% 0
			background-repeat no-repeat
		&._style-2
			background-size cover
			background-position 0 0
			background-repeat no-repeat
			background-color #2076dd
		&._style-3
			background-size cover
			background-position 0 0
			background-repeat no-repeat
			background-color #2076dd
		&._style-4
			background-size cover
			background-position 0 0
			background-repeat no-repeat
			background-color #2076dd
		&._style-5
			background-size cover
			background-position 0 0
			background-repeat no-repeat
			background-color #2076dd
	&-title
		position relative
		margin-top 13.75rem
		font normal 3rem/1 $ff-mb
		color #fff
		text-transform uppercase
		&:before
			position absolute
			top -8.125rem
			left 50%
			margin-left -1.8125rem
			content: ""
			sprite($rocket)
			@media all and (-webkit-min-device-pixel-ratio: 1.5)
				sprite2($rocket_2x)
		@media all and (max-width 20rem)
			font normal 1.625rem/1 $ff-mb

	&-slogan
		margin-bottom 3.625rem
		font normal 1rem/1.4 $ff-or
	&-buttons
		position relative
		font normal 0/1 $ff-mb
		text-transform uppercase
	&-button
		&-i
			padding 0 2.5rem
			display inline-block
			line-height 3.75rem
			text-decoration none
			font-size 0.875rem
			color #FFF
			&._style-1
				background #59a3fc
				border-radius 0.625rem 0 0 0.625rem
			&._style-2
				background #3d92f9
				border-radius 0 0.625rem 0.625rem 0
			@media all and (max-width 20rem)
				padding 0 1.25rem
				font-size 0.75rem
				line-height 2.0625rem
			@media all and (min-width 20.0625rem)
				padding 0 (40/989)*100%
	&-buttons
		&-sep
			position absolute
			top 1.0625rem
			left 50%
			margin-left -0.875rem
			width 1.75rem
			background #0078B2
			font-size 0.75rem
			line-height 1.75rem
			border-radius 999px
			@media all and (max-width 20rem)
				top 0.1875rem

	&-pag
		position absolute
		left 0
		right 0
		bottom 6.25rem
		font-size 0
		text-align center
		z-index 99
		.bx-pager
			padding 0
			display inline-block
		.bx-pager-item
			margin-left 1.125rem
			display inline-block
			&:first-child
				margin-left 0
		.bx-pager-link
			display inline-block
			size 0.75rem
			background #636363
			text-indent -9999px
			-webkit-border-radius 999px
			border-radius 999px
		.bx-pager-link
			&:hover
				.bx-pager-link
					background #B7B7B7
		.bx-pager-link.active
				background #B7B7B7
		@media all and (max-width 20rem)
			bottom 7.5rem
		@media all and (min-width 20.0625rem)
			bottom 1.25rem
		@media all and (min-width 47.5rem)
			bottom 6.25rem
